<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bmf.infrastructure.dal.mapper.DomainMapper">

    <sql id="Base_Column_List">
        id, domain_name, domain_type, domain_level
    </sql>

    <select id="selectOne" resultType="com.bmf.infrastructure.dal.po.DomainPO">
        SELECT
        <include refid="Base_Column_List"/>
        FROM bmf_domain
    </select>

    <insert id="insert" parameterType="com.bmf.infrastructure.dal.po.DomainPO" >
        insert into bmf_domain (domain_name, domain_type, domain_level)
        values (
            #{domainName,jdbcType=VARCHAR},
            #{domainType,jdbcType=VARCHAR},
            #{domainLevel,jdbcType=BIGINT}
        )
    </insert>

</mapper>